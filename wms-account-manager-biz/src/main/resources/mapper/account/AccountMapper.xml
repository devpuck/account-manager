    </sql>
    <update id="inMergeByCode">
        update WMS_ACCOUNT set QUANTITY = QUANTITY + #{quantity} where ACCOUNT_CODE = #{code}
    </update>
    <update id="inMergeByID">
        update WMS_ACCOUNT set QUANTITY = QUANTITY + #{quantity} where ID = #{id}
    </update>

    <select id="getAccountById" resultMap="BoResultMap">
        select
        <include refid="Base_Column_List"/>
        from WMS_ACCOUNT where id = #{id}
    </select>

    <select id="getAccountPageList" resultMap="BoResultMap">
        select
        <include refid="Base_Column_List"/>
        from WMS_ACCOUNT
    </select>
    <select id="queryAccountByWarehouse" resultType="com.wms.model.bo.account.AccountBo">
        select
        <include refid="Base_Column_List"/>
        from WMS_ACCOUNT where WAREHOUSE_CODE = #{warehouseCode} and QUANTITY >0
    </select>

